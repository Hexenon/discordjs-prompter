{
  "name": "discordjs-prompter",
  "version": "1.1.0",
  "description": "Prompt for a user response using reactions or a massage.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "run-s build:* && jest --verbose && npm run lint:check",
    "build:clean": "rimraf ./dist",
    "build:typescript": "tsc --pretty",
    "build:declaration": "dts-bundle-generator ./src/index.ts -o ./dist/index.d.ts --external-imports=discord.js",
    "build": "run-s build:*",
    "prepack": "run-s build:*",
    "lint:check": "eslint ./src/**/*",
    "lint:fix": "eslint ./src/**/* --fix"
  },
  "keywords": [
    "discord",
    "prompt",
    "discordjs",
    "bot",
    "question",
    "confirm"
  ],
  "author": "Joaquim Neto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimnet/discordjs-prompter.git"
  },
  "homepage": "https://github.com/joaquimnet/discordjs-prompter",
  "bugs": "https://github.com/joaquimnet/discordjs-prompter/issues",
  "dependencies": {},
  "peerDependencies": {
    "discord.js": "^11.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.7",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-jest": "^24.8.0",
    "discord.js": "^11.5.1",
    "dts-bundle-generator": "^3.1.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  }
}
